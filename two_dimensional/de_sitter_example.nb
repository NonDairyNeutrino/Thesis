Cell["\<\
The phase and density in the Madelung representation of the particle field \
can be Fourier expanded with the following amplitudes.  In other words, the \
phase and density of the particle field can be represented via the inverse \
discrete Fourier transform with the following amplitudes.\
\>", "Text",
 CellChangeTimes->{{3.8902721184193544`*^9, 
  3.8902722438843327`*^9}},ExpressionUUID->"d81b3944-8322-4234-970e-\
fc5f6bc7ec31"],

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"phaseFourierAmplitude", ",", "densityFourierAmplitude"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"eq", " ", "82"}], "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"phaseFourierAmplitude", "[", 
     RowBox[{"\"\<out\>\"", ",", "timeScale_"}], "]"}], "[", 
    RowBox[{"labTime_", ",", "waveNumber_"}], "]"}], ":=", 
   RowBox[{"With", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"dispersion", "=", 
       RowBox[{
        RowBox[{"dispersion", "[", "timeScale", "]"}], "[", 
        RowBox[{"labTime", ",", "waveNumber"}], "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{
        RowBox[{"interactionStrength", "[", "labTime", "]"}], "/", 
        RowBox[{"(", 
         RowBox[{"2", "\[HBar]", " ", "dispersion"}], ")"}]}], "]"}], 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "I"}], " ", "dispersion", " ", "labTime"}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"eq", " ", "83"}], "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"densityFourierAmplitude", "[", 
     RowBox[{"\"\<out\>\"", ",", "timeScale_"}], "]"}], "[", 
    RowBox[{"labTime_", ",", "waveNumber_"}], "]"}], ":=", 
   RowBox[{"With", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"dispersion", "=", 
       RowBox[{
        RowBox[{"dispersion", "[", "timeScale", "]"}], "[", 
        RowBox[{"labTime", ",", "waveNumber"}], "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"I", " ", 
      RowBox[{"Sqrt", "[", 
